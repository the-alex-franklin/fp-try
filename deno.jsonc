{
	"name": "@2or3godzillas/fp-try",
	"version": "1.4.0",
	"exports": "./mod.ts",
	"description": "Functional programming try-catch",
	"compilerOptions": {
		"lib": ["ES2018", "DOM"]
	},
	"lint": {
		"rules": {
			"exclude": [
				"no-explicit-any",
				"no-dupe-class-members",
				"adjacent-overload-signatures",
				"require-await",
				"no-unused-vars"
			]
		}
	},
	"fmt": {
		"lineWidth": 100,
		"semiColons": true,
		"useTabs": true
	}
}
